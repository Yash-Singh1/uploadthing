{
  "name": "@uploadthing/react-shared",
  "version": "6.2.2",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "react-native": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "react-native": "./dist/types.mjs"
    }
  },
  "browser": "./dist/index.mjs",
  "description": "Internal shared utilities between the React and React Native SDKs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@uploadthing/shared": "file:~/uploadthing-redo/uploadthing/packages/shared"
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@uploadthing/eslint-config": "0.2.0",
    "@uploadthing/tsconfig": "0.1.0",
    "@uploadthing/tsup-config": "0.1.0",
    "eslint": "^8.56.0",
    "react": "18.2.0",
    "tsup": "6.7.0",
    "typescript": "^5.3.3",
    "uploadthing": "6.3.1"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0",
    "uploadthing": "^6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@uploadthing/eslint-config/base",
      "@uploadthing/eslint-config/react"
    ]
  }
}
