{
  "name": "@uploadthing/react-native",
  "version": "6.2.2",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "react-native": "./dist/index.mjs"
    }
  },
  "react-native": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "wait-on ../uploadthing/dist/server.mjs && tsup --watch"
  },
  "dependencies": {
    "@uploadthing/react-shared": "file:~/uploadthing-redo/uploadthing/packages/react-shared",
    "@uploadthing/shared": "^6.2.0",
    "attr-accept": "^2.2.2",
    "file-selector": "^0.6.0",
    "react-native-svg": "^14.1.0",
    "tailwind-merge": "^1.14.0"
  },
  "peerDependencies": {
    "expo-document-picker": "^11.10.1",
    "expo-file-system": "^16.0.5",
    "expo-image-picker": "^14.7.1",
    "react": "^17.0.2 || ^18.0.0",
    "react-native": "*",
    "uploadthing": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "expo-document-picker": {
      "optional": true
    },
    "expo-image-picker": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "@types/react": "18.2.33",
    "@uploadthing/eslint-config": "0.2.0",
    "@uploadthing/tsconfig": "0.1.0",
    "@uploadthing/tsup-config": "0.1.0",
    "eslint": "^8.56.0",
    "expo-document-picker": "^11.10.1",
    "expo-file-system": "^16.0.5",
    "expo-image-picker": "^14.7.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "tailwindcss": "^3.4.1",
    "tsup": "6.7.0",
    "typescript": "^5.3.3",
    "uploadthing": "6.3.1",
    "vitest": "^1.2.2",
    "wait-on": "^7.2.0",
    "zod": "^3.22.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@uploadthing/eslint-config/base",
      "@uploadthing/eslint-config/react"
    ]
  }
}
