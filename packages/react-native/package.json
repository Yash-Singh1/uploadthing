{
  "name": "@uploadthing/react-native",
  "author": "Yash Singh",
  "license": "MIT",
  "version": "3.0.4",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "browser": {
    "./package.json": "./package.json",
    ".": "./dist/index.mjs",
    "./hooks": "./dist/hooks.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint *.ts* --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "expo-document-picker": "*",
    "expo-image-picker": "*",
    "react": "^17.0.2 || ^18.0.0",
    "react-native": "*",
    "uploadthing": "3.0.4"
  },
  "devDependencies": {
    "@types/node": "18.16.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.0.11",
    "@uploadthing/eslint-config": "0.1.0",
    "@uploadthing/shared": "0.1.0",
    "eslint": "^8.40.0",
    "expo-document-picker": "^11.2.2",
    "expo-image-picker": "^14.1.1",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "tsup": "6.7.0",
    "typescript": "5.1.0-beta",
    "uploadthing": "3.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "directory": "packages/react-native",
    "url": "github:pingdotgg/uploadthing"
  }
}
